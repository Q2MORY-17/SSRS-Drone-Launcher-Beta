pipeline {
  agent any
  stages {
    stage('Checkout') {
      steps {
        git 'https://github.com/2MORY17/SSRS-Drone-Launcher-Beta'
      }
    }
    
    stage('Unit test') {
      steps {
        sh 'python3 -m pytest test/manual/options/unittest/test_dronelauncher_python.py'
      }
    }
  }
}